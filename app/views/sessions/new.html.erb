<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= t('activerecord.attributes.user.email') %><br>
  <%= f.email_field :email %><br>
  <%= t('activerecord.attributes.user.password') %><br>
  <%= f.password_field :password %><br>
  <br>
  <%= f.submit "Log in" %>
<% end %>